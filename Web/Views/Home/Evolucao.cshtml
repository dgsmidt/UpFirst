@using DAL.Models
@model List<Nota>

@{
    ViewData["Title"] = "Evolucao";
}

<h1>Evolução</h1>
<div class="mt-4">
    @if (Model.Count == 0)
    {
        <h5>Ainda não há notas lançadas</h5>
    }
    @foreach (var item in Model)
    {
        <h6>@item.Modulo.Descricao - @item.Valor</h6>
    }
</div>

